{
    "name": "@workleap/tsup-configs",
    "author": "Workleap",
    "description": "Workleap's recommended tsup configurations.",
    "version": "3.1.0",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/workleap/wl-web-configs.git",
        "directory": "packages/tsup-configs"
    },
    "type": "module",
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "src",
        "dist",
        "CHANGELOG.md"
    ],
    "scripts": {
        "build": "rslib build -c rslib.config.ts",
        "eslint": "eslint . --max-warnings=-0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsc",
        "test": "vitest --config vitest.config.ts --no-watch"
    },
    "peerDependencies": {
        "tsup": "^8.5.0",
        "typescript": "^5.9.3"
    },
    "devDependencies": {
        "@eslint/js": "9.38.0",
        "@rsbuild/core": "1.5.17",
        "@rslib/core": "0.16.0",
        "@typescript-eslint/parser": "8.46.2",
        "@workleap/eslint-configs": "workspace:*",
        "@workleap/rslib-configs": "workspace:*",
        "@workleap/typescript-configs": "3.0.4",
        "eslint": "9.38.0",
        "typescript": "5.9.3",
        "typescript-eslint": "8.46.2",
        "vitest": "4.0.1"
    }
}
