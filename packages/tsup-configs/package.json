{
    "name": "@workleap/tsup-configs",
    "author": "Workleap",
    "description": "Workleap's recommended tsup configurations.",
    "version": "3.0.8",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/workleap/wl-web-configs.git",
        "directory": "packages/tsup-configs"
    },
    "type": "module",
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "src",
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "build": "rslib build -c rslib.config.ts",
        "eslint": "eslint . --max-warnings=-0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsc",
        "test": "vitest --config vitest.config.ts --no-watch"
    },
    "peerDependencies": {
        "tsup": "*",
        "typescript": "*"
    },
    "devDependencies": {
        "@rsbuild/core": "1.2.19",
        "@rslib/core": "0.5.4",
        "@typescript-eslint/parser": "8.26.1",
        "@workleap/eslint-plugin": "workspace:*",
        "@workleap/rslib-configs": "workspace:*",
        "@workleap/typescript-configs": "workspace:*",
        "eslint": "8.57.0",
        "typescript": "5.8.2",
        "vitest": "3.0.9"
    }
}
