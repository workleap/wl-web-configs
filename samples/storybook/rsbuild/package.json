{
    "name": "@storybook-sample/rsbuild",
    "description": "Storybook app using Rsbuild as the bundler.",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "storybook dev -p 6006",
        "build": "storybook build",
        "serve-build": "pnpm http-server storybook-static -p 8080 -P http://localhost:8080? -c-1",
        "eslint": "eslint . --max-warnings=0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsc"
    },
    "dependencies": {
        "react": "19.2.1",
        "react-dom": "19.2.1"
    },
    "devDependencies": {
        "@eslint/js": "9.39.1",
        "@rsbuild/core": "1.6.14",
        "@rspack/core": "1.6.7",
        "@storybook/addon-a11y": "10.1.6",
        "@storybook/addon-links": "10.1.6",
        "@storybook/react": "10.1.6",
        "@types/react": "19.2.7",
        "@types/react-dom": "19.2.3",
        "@typescript-eslint/parser": "8.49.0",
        "@workleap/browserslist-config": "workspace:*",
        "@workleap/eslint-configs": "workspace:*",
        "@workleap/rsbuild-configs": "workspace:*",
        "@workleap/typescript-configs": "workspace:*",
        "browserslist": "4.28.1",
        "eslint": "9.39.1",
        "http-server": "14.1.1",
        "storybook": "10.1.6",
        "storybook-react-rsbuild": "3.1.0",
        "typescript": "5.9.3",
        "typescript-eslint": "8.49.0"
    }
}
